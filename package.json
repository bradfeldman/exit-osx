{
  "name": "exit-osx",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:staging": "TEST_BASE_URL=https://staging.exitosx.com playwright test",
    "test:e2e:prod": "TEST_BASE_URL=https://app.exitosx.com playwright test",
    "test:e2e:local": "TEST_BASE_URL=http://localhost:3000 playwright test",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:visual": "playwright test e2e/tests/visual-regression.spec.ts",
    "test:visual:update": "playwright test e2e/tests/visual-regression.spec.ts --update-snapshots",
    "test:visual:no-auth": "playwright test --project=no-auth e2e/tests/visual-regression.spec.ts",
    "test:perf": "playwright test e2e/tests/performance.spec.ts",
    "test:perf:summary": "playwright test e2e/tests/performance.spec.ts --grep @smoke",
    "test:perf:no-auth": "playwright test --project=no-auth e2e/tests/performance.spec.ts",
    "test:load": "k6 run load-tests/api-load-test.js",
    "test:load:smoke": "k6 run load-tests/smoke-test.js",
    "test:load:stress": "k6 run load-tests/stress-test.js",
    "test:load:spike": "k6 run load-tests/spike-test.js",
    "test:load:soak": "k6 run load-tests/soak-test.js",
    "test:load:local": "BASE_URL=http://localhost:3000 k6 run load-tests/api-load-test.js",
    "test:load:staging": "BASE_URL=https://staging.exitosx.com k6 run load-tests/api-load-test.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server ANALYZE=true npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser ANALYZE=true npm run build"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "intuit-oauth": "^4.2.2",
    "lucide-react": "^0.562.0",
    "next": "^15.1.0",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "resend": "^6.7.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@next/bundle-analyzer": "^16.1.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "csv-parse": "^6.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.17",
    "vitest-axe": "^0.1.0"
  }
}
